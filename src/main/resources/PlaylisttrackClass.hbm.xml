<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Aplicacion.PlaylisttrackClass" table="playlisttrack" schema="chinook">
        <composite-id mapped="true" class="Aplicacion.PlaylisttrackClassPK">
            <key-property name="playlistId" column="PlaylistId"/>
            <key-property name="trackId" column="TrackId"/>
        </composite-id>
        <many-to-one name="playlistByPlaylistId" class="Aplicacion.PlaylistClass">
            <column name="PlaylistId"/>
        </many-to-one>
    </class>
</hibernate-mapping>